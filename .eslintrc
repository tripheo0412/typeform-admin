{
  "extends": ["wesbos"],
  "rules": {
    "import/no-named-as-default": 0,
    "no-console": 1,
    "prefer-destructuring": ["error", { "object": true, "array": true }],
    "jsx-a11y/label-has-for": [
      "error",
      {
        "required": {
          "some": ["nesting", "id"]
        }
      }
    ]
  }
}
